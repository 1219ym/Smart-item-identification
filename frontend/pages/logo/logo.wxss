/* logo.wxss */
.logo-container {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100vh;
  width: 100vw;
  background-color: #fff;
}

.logo-title {
  font-size: 18px;
  color: #333;
  margin-bottom: 20px;
}

.logo-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.enter-button {
  position: absolute;
  top: 650px; /* 调整按钮距离顶部的距离，根据需要调整 */
  left: 50%;
  transform: translateX(-50%);
  padding: 10px 20px;
  background-color: #3498db;
  color: #fff;
  border: none;
  border-radius: 5px;
  font-size: 16px;
  cursor: pointer;
}





